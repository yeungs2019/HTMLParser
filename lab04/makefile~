all: main.o sums.o 
	g++ -g obj/main.o obj/sums.o -o all

main.o: src/main.cpp obj
	g++ -c src/main.cpp -o obj/main.o

sums.o: src/sums.cpp obj src/sums.h
	g++ -c src/sums.cpp -o obj/sums.o

obj:
	mkdir -p obj

clean:
	rm -rf obj
	rm -f all

plot: timings.plot data1 data2 data3 data4 data5
	gnuplot < timings.plot
	ps2pdf output.ps
	evince output.pdf
